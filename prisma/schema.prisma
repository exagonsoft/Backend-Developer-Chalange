// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Make {
  makeId      Int            @id @default(autoincrement())
  makeName    String
  vehicleTypes VehicleType[]
}

model VehicleType {
  typeId   Int    @id @default(autoincrement())
  typeName String
  makeId   Int
  Make     Make   @relation(fields: [makeId], references: [makeId])
}
